<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Ellipse?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="480.0" prefWidth="448.0"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="main.controller.AdminReportController">
    <children>
        <ToolBar layoutX="1.0" prefHeight="45.0" prefWidth="448.0">
            <items>
                <Button mnemonicParsing="false" onAction="#HomePage" prefHeight="31.0" prefWidth="70.0" text="Home"/>
                <Button mnemonicParsing="false" onAction="#ProfilePage" prefHeight="31.0" prefWidth="71.0"
                        text="Profile"/>
                <Button mnemonicParsing="false" onAction="#Logout" prefHeight="31.0" prefWidth="76.0" text="Logout"/>
            </items>
        </ToolBar>
        <Label layoutX="142.0" layoutY="118.0">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <Label layoutX="187.0" layoutY="136.0" prefHeight="35.0" prefWidth="76.0" text="Report">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <Button layoutX="142.0" layoutY="304.0" mnemonicParsing="false" onAction="#EmployeeReport" prefHeight="31.0"
                prefWidth="167.0" text="Employee"/>
        <Circle fill="WHITE" layoutX="50.0" layoutY="81.0" radius="18.0" stroke="BLACK" strokeType="INSIDE"/>
        <Circle layoutX="50.0" layoutY="76.0" radius="5.0" stroke="BLACK" strokeType="INSIDE"/>
        <Ellipse layoutX="50.0" layoutY="90.0" radiusX="13.0" radiusY="8.0" stroke="BLACK" strokeType="INSIDE"/>
        <Label fx:id="username" layoutX="88.0" layoutY="71.0" text="Label"/>
        <Button onAction="#BookingReport" layoutX="141.0" layoutY="225.0" mnemonicParsing="false" prefHeight="31.0"
                prefWidth="167.0" text="Booking"/>
        <Button onAction="#Back" layoutX="359.0" layoutY="67.0" mnemonicParsing="false" text="&lt;Back"/>
    </children>
</AnchorPane>
